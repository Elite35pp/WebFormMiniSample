@model IEnumerable<int>

@{
    this.Layout = null;
    string test = "Hello world!";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        @test
    </div>
    <div>
        @this.ViewBag.MyParams
    </div>

    @foreach(int i in this.Model)
            {
                <p> @i  </p>
            }
</body>
</html>